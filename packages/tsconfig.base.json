{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "resolveJsonModule": true,
        "declaration": true,
        "esModuleInterop": true,
        "composite": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": false,
        "forceConsistentCasingInFileNames": false,
        "skipLibCheck": true,
        "strict": false
    }
}